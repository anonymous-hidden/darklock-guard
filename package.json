{
  "name": "darklock-security-bot",
  "version": "1.0.1",
  "description": "Advanced DarkLock with comprehensive protection features",
  "main": "src/bot.js",
  "scripts": {
    "start": "node src/bot.js",
    "build": "echo 'no build required'",
    "postinstall": "node file-protection/agent/baseline-generator.js",
    "dev": "nodemon src/bot.js",
    "setup": "node setup.js",
    "fix-encoding": "node fix-all-mojibake.cjs",
    "test:seed-preserve": "node scripts/test-seed-preserve.js",
    "heroku-postbuild": "echo 'Build complete'",
    "security:audit": "npm audit",
    "security:fix": "npm audit fix",
    "security:check": "npm audit --audit-level=moderate",
    "tamper:generate": "node file-protection/agent/baseline-generator.js",
    "tamper:test": "node file-protection/test.js"
  },
  "engines": {
    "node": ">=18",
    "npm": ">=8"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "bcrypt": "^5.1.1",
    "canvas": "^2.11.2",
    "chokidar": "^5.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "geoip-lite": "^1.4.8",
    "helmet": "^7.1.0",
    "is-arrayish": "^0.3.4",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.29.4",
    "ms": "^2.1.3",
    "multer": "^2.0.2",
    "node-cron": "^3.0.3",
    "nodemailer": "^7.0.11",
    "qrcode": "^1.5.3",
    "qs": ">=6.14.1",
    "sharp": "^0.33.5",
    "speakeasy": "^2.0.0",
    "sqlite3": "^5.1.7",
    "string-similarity": "^4.0.4",
    "stripe": "^16.6.0",
    "url-parse": "^1.5.10",
    "winston": "^3.11.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "keywords": [
    "discord",
    "bot",
    "security",
    "anti-raid",
    "anti-spam",
    "moderation"
